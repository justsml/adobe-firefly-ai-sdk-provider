{
	"name": "adobe-firefly-ai-sdk",
	"version": "1.0.0",
	"description": "AI SDK provider for Adobe Firefly image generation models",
	"type": "module",
	"private": false,
	"sideEffects": false,
	"license": "MIT",
	"author": "",
	"repository": {
		"type": "git",
		"url": "https://github.com/username/adobe-firefly-ai-sdk.git"
	},
	"bugs": {
		"url": "https://github.com/username/adobe-firefly-ai-sdk/issues"
	},
	"homepage": "https://github.com/username/adobe-firefly-ai-sdk#readme",
	"keywords": [
		"ai",
		"adobe",
		"firefly",
		"image-generation",
		"ai-sdk",
		"generative-ai"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build",
		"test": "vitest --run",
		"test:coverage": "vitest --coverage --run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"prepare": "npm run build",
		"prepublishOnly": "npm run lint && npm run test && npm run typecheck",
		"clean": "rimraf ./dist ./build ./tsconfig.tsbuildinfo ./node_modules",
		"typecheck": "tsc -p tsconfig.json --noEmit"
	},
	"dependencies": {
		"@ai-sdk/provider": "^2.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"glob": "^11.0.3",
		"rimraf": "^6.0.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vite-plugin-dts": "^4.5.4",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.8"
	},
	"peerDependencies": {
		"@ai-sdk/provider": "^2.0.0"
	}
}
