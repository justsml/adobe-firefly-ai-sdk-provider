{
	"name": "adobe-firefly-ai-sdk",
	"version": "1.0.0",
	"type": "module",
	"private": false,
	"sideEffects": false,
	"main": "./dist/index.js",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"files": [
		"src",
		"dist"
	],
	"exports": {
		".": {
			"types": "./src/index.ts",
			"require": "./dist/index.js",
			"import": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build",
		"test": "vitest --run",
		"test:coverage": "vitest --coverage --run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"postinstall": "if [ -z \"$CI\" ]; then biome check --write ./package.json; fi",
		"clean": "rimraf ./dist ./build ./tsconfig.tsbuildinfo ./node_modules",
		"typecheck": "tsc -p tsconfig.json --noEmit"
	},
	"dependencies": {
		"@ai-sdk/provider": "^2.0.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"glob": "^11.0.3",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vite-plugin-dts": "^4.5.4",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.8"
	},
	"peerDependencies": {
		"@ai-sdk/provider": "^2.0.0"
	}
}
